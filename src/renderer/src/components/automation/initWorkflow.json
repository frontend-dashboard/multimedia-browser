{
  "id": "init_workflow",
  "name": "RPA初始化演示工作流",
  "description": "包含所有支持的RPA元件类型的演示工作流",
  "createdAt": "2023-11-08T00:00:00.000Z",
  "updatedAt": "2023-11-08T00:00:00.000Z",
  "elements": [
    {
      "id": "element_browser_open",
      "type": "BROWSER_OPEN",
      "name": "打开浏览器",
      "category": "browser",
      "position": {"x": 100, "y": 50},
      "params": {
        "url": "https://www.example.com",
        "openMode": "newBrowser",
        "browserType": "chrome",
        "incognito": false,
        "windowSize": "maximized",
        "waitUntil": "networkidle",
        "timeout": 30000
      }
    },
    {
      "id": "element_browser_navigate",
      "type": "BROWSER_NAVIGATE",
      "name": "导航到URL",
      "category": "browser",
      "position": {"x": 300, "y": 50},
      "params": {
        "url": "https://www.baidu.com",
        "waitForLoad": true
      }
    },
    {
      "id": "element_input_text",
      "type": "INPUT_TEXT",
      "name": "输入文本",
      "category": "interaction",
      "position": {"x": 500, "y": 50},
      "params": {
        "selector": "#kw",
        "text": "三字经的正规全文",
        "clearBefore": true
      }
    },
    {
      "id": "element_click_search",
      "type": "CLICK_ELEMENT",
      "name": "点击搜索",
      "category": "interaction",
      "position": {"x": 700, "y": 50},
      "params": {
        "selector": "#su",
        "waitForNavigation": true,
        "clickCount": 1
      }
    },
    {
      "id": "element_wait_search",
      "type": "WAIT",
      "name": "等待搜索结果",
      "category": "logic",
      "position": {"x": 100, "y": 150},
      "params": {
        "seconds": 2
      }
    },
    {
      "id": "element_click_tab",
      "type": "CLICK_ELEMENT",
      "name": "点击标签",
      "category": "interaction",
      "position": {"x": 300, "y": 150},
      "params": {
        "selector": "#content_left > .s-tab-item:nth-child(2)",
        "waitForNavigation": true,
        "clickCount": 1
      }
    },
    {
      "id": "element_hover_element",
      "type": "HOVER_ELEMENT",
      "name": "悬停元素",
      "category": "interaction",
      "position": {"x": 500, "y": 150},
      "params": {
        "selector": "#content_left .result:first-child"
      }
    },
    {
      "id": "element_scroll_page",
      "type": "SCROLL_PAGE",
      "name": "滚动页面",
      "category": "interaction",
      "position": {"x": 700, "y": 150},
      "params": {
        "scrollType": "down",
        "selector": ""
      }
    },
    {
      "id": "element_extract_data",
      "type": "EXTRACT_DATA",
      "name": "提取数据",
      "category": "data",
      "position": {"x": 100, "y": 250},
      "params": {
        "selector": "#content_left",
        "extractType": "text",
        "variableName": "pageData"
      }
    },
    {
      "id": "element_get_page_elements",
      "type": "GET_PAGE_ELEMENTS",
      "name": "获取页面元素",
      "category": "data",
      "position": {"x": 300, "y": 250},
      "params": {
        "browserId": "",
        "selector": ".result",
        "extractDetails": true,
        "variableName": "searchResults"
      }
    },
    {
      "id": "element_process_data",
      "type": "PROCESS_DATA",
      "name": "处理数据",
      "category": "data",
      "position": {"x": 500, "y": 250},
      "params": {
        "inputVariable": "pageData",
        "operationType": "trim",
        "outputVariable": "processedData"
      }
    },
    {
      "id": "element_compare_data",
      "type": "COMPARE_DATA",
      "name": "比较数据",
      "category": "data",
      "position": {"x": 700, "y": 250},
      "params": {
        "value1": "pageData",
        "value2": "",
        "operator": "notEqual",
        "resultVariable": "hasData"
      }
    },
    {
      "id": "element_if_condition",
      "type": "IF_CONDITION",
      "name": "条件判断",
      "category": "logic",
      "position": {"x": 100, "y": 350},
      "params": {
        "condition": "hasData === true",
        "trueBranchId": "",
        "falseBranchId": ""
      }
    },
    {
      "id": "element_loop_for",
      "type": "LOOP_FOR",
      "name": "循环",
      "category": "logic",
      "position": {"x": 300, "y": 350},
      "params": {
        "loopCount": 2,
        "loopVariable": "i"
      }
    },
    {
      "id": "element_try_catch",
      "type": "TRY_CATCH",
      "name": "异常处理",
      "category": "logic",
      "position": {"x": 500, "y": 350},
      "params": {
        "errorVariable": "error"
      }
    },
    {
      "id": "element_read_file",
      "type": "READ_FILE",
      "name": "读取文件",
      "category": "file",
      "position": {"x": 700, "y": 350},
      "params": {
        "filePath": "~/Desktop/sample.txt",
        "encoding": "utf8",
        "outputVariable": "fileContent"
      }
    },
    {
      "id": "element_write_file",
      "type": "WRITE_FILE",
      "name": "写入文件",
      "category": "file",
      "position": {"x": 100, "y": 450},
      "params": {
        "filePath": "~/Desktop/output.txt",
        "content": "${processedData}",
        "encoding": "utf8",
        "append": false
      }
    },
    {
      "id": "element_save_file",
      "type": "SAVE_FILE",
      "name": "保存文件",
      "category": "file",
      "position": {"x": 300, "y": 450},
      "params": {
        "data": "${pageData}",
        "filePath": "~/Desktop/国庆搜索结果.txt",
        "format": "txt"
      }
    },
    {
      "id": "element_download_file",
      "type": "DOWNLOAD_FILE",
      "name": "下载文件",
      "category": "file",
      "position": {"x": 500, "y": 450},
      "params": {
        "url": "https://example.com/sample.pdf",
        "savePath": "~/Downloads/sample.pdf"
      }
    },
    {
      "id": "element_upload_file",
      "type": "UPLOAD_FILE",
      "name": "上传文件",
      "category": "file",
      "position": {"x": 700, "y": 450},
      "params": {
        "selector": "input[type=file]",
        "filePath": "~/Desktop/upload.txt"
      }
    },
    {
      "id": "element_browser_refresh",
      "type": "BROWSER_REFRESH",
      "name": "刷新页面",
      "category": "browser",
      "position": {"x": 300, "y": 550},
      "params": {
        "waitForLoad": true
      }
    },
    {
      "id": "element_browser_close",
      "type": "BROWSER_CLOSE",
      "name": "关闭浏览器",
      "category": "browser",
      "position": {"x": 500, "y": 550},
      "params": {}
    },
    {
      "id": "element_select_option",
      "type": "SELECT_OPTION",
      "name": "选择选项",
      "category": "interaction",
      "position": {"x": 100, "y": 550},
      "params": {
        "selector": "select",
        "optionText": "Option 1",
        "optionValue": "option1"
      }
    }
  ],
  "edges": [
